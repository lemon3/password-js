{
  "name": "pwkit",
  "private": false,
  "version": "0.0.6",
  "packageManager": "pnpm@9.11.0",
  "description": "Modern, customizable password generator with entropy analysis and flexible character group settings.",
  "keywords": [
    "password generator",
    "secure password",
    "entropy",
    "random password",
    "pwgen",
    "password strength",
    "password entropy",
    "char map",
    "custom password",
    "crypto safe",
    "password tool",
    "typescript password generator",
    "password complexity",
    "frontend security",
    "generate passwords"
  ],
  "author": "wolfgang jungmayer",
  "license": "MIT",
  "browser": "./dist/pwkit.umd.js",
  "type": "module",
  "main": "./dist/pwkit.umd.js",
  "module": "./dist/pwkit.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/pwkit.js",
      "require": "./dist/pwkit.umd.js"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "tsc && vite build",
    "demo": "vite build --config vite.config.demo.js",
    "preview": "vite preview",
    "coverage": "vitest --coverage"
  },
  "homepage": "https://github.com/lemon3/pwkit",
  "devDependencies": {
    "@vitest/coverage-v8": "3.1.2",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.3",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.2"
  }
}
